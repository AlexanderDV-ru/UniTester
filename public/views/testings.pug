extends /headered

block title
	title #{$msgs$.testings} - #{$msgs$.sitename}

block inHead0
	link(rel='stylesheet', href='/stylesheets/style2.css')

block content
	- for(var v=0;v<data.testingsInfo.length;v++)
		label #{data.testingsInfo[v].id}
		button
			a.nontexthref(href="/testing?id="+data.testingsInfo[v].id+"&questionNumber=0") #{$msgs$.openTesting}
		button
			a.nontexthref(href="/testingResult?id="+data.testingsInfo[v].id+"&questionNumber=0") #{$msgs$.getResult}